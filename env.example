PG_HOST=localhost
PG_PORT=5432
PG_DBNAME=your_database_name
PG_USER=your_database_user
PG_PASSWORD=your_database_password

# OAuth-клиент Google (из Google Cloud Console, OAuth 2.0 Client ID, Desktop app)
# Эти два значения можно просто скопировать из консоли и вставить сюда.
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Файл, куда будет сохранён токен после первого входа через браузер
GOOGLE_TOKEN_FILE=token.json

# (Необязательно) ID папки на Google Диске, куда складывать копии таблиц.
# Если оставить пустым, файлы будут создаваться в корне диска.
GOOGLE_DESTINATION_FOLDER_ID=

# Имя выходного файла с маппингом клиентов и ссылок
MAPPING_FILE=mapping.json

